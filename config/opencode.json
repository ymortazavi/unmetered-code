{
  "$schema": "https://opencode.ai/config.json",
  "provider": {
    "litellm": {
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "http://litellm:4000/v1"
      },
      "models": {
        "minimax-m2.5": {
          "name": "minimax-m2.5"
        }
      }
    }
  },
  "model": "litellm/minimax-m2.5",
  "autoupdate": false,
  "mcp": {
    "search": {
      "type": "local",
      "command": ["mcp-server-searxng"],
      "environment": {
        "SEARXNG_INSTANCES": "http://searxng-umcode:8080"
      },
      "timeout": 15000,
      "enabled": true
    }
  }
}
